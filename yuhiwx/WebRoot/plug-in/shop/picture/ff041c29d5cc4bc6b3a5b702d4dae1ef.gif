    String.prototype.trim=function(){return this.replace(/(^\s*)|(\s*$)/g, "");}
　　 String.prototype.ltrim=function(){return this.replace(/(^\s*)/g,"");}
　　 String.prototype.rtrim=function(){ return this.replace(/(\s*$)/g,"");}
	
    function productSearch(){
		$('#search-keyword').focus();
		if($('#search-keyword').val() == null || $('#search-keyword').val().trim() == '' || '输入关键词' == $('#search-keyword').val())
		{
		   $('#search-keyword').val('');
		   alert('请输入查询条件！');
		}
		else
		{
		   $('#searchForm').attr('action',applicationContextPath + '/product/productSearchList.action');
		   $('#searchForm').submit();
		}
    }
    
    function quickSearch(keyword){
    	$('#search-keyword').val(keyword);
    	productSearch();
    }
	
	//获取购物车中商品的数量    
    function getCookieProductAmount(){
    	$.ajax({
		   type: "POST",
		   url: "/mall" + "/cart/getCookieProductAmount.action",
		   data : "",
		   success: function(msg){
		   	  $("#cookie_product_amount").html(msg);
		   }
		});
    	
    }
    
	//获取信用卡积分、商城积分、信用卡名称、商城积分名称和用户昵称
	function getTopBonus(){
		$.ajax({
			   type: "POST",
			   url: "/mall" + "/member/credit/ajaxGetMemberInfo.action",
			   data : "",
			   dataType:'json',
			   success: function(msg){
				   if("1" == msg[0].status){
						$("#creditBonusName").html(msg[0].creditBonusName + "积分：<span>"+msg[0].bankBonus+"分</span>");
						$("#mallBonusName").html(msg[0].ecomBonusName + "：<span>"+msg[0].ecomBonus+"分</span>");
						$("#changeInfo").html("我能换什么");
						$("#changeInfo").attr('href','#');
						$("#pointRange").val('1-'+(parseInt(msg[0].ecomBonus)+parseInt(msg[0].bankBonus)));
						$("#changeInfo").click(
						function(){
							$("#qryPointRange").submit();
						});
				   }else{
						$("#creditBonusName").html(msg[0].creditBonusName + "积分：<span>"+0+"分</span>");
						$("#mallBonusName").html(msg[0].ecomBonusName + "：<span>"+0+"分</span>");
				   }
			   }
			});
	}
	
	
    function searchKeyDown(event){
    	var e = event || window.event || arguments.callee.caller.arguments[0];
        if(e && e.keyCode==13){ // enter 键
        	if($('#search-keyword').val() == null || $('#search-keyword').val().trim() == '' || '输入关键词' == $('#search-keyword').val()){
        		
        	}else{
        		productSearch();
        	}
        }
    }
    
    function validateSearch(){
    	if($('#search-keyword').val() == null || $('#search-keyword').val().trim() == '' || '输入关键词' == $('#search-keyword').val()){
        	return false;
        }
        return true;
    }
    
    function initMenu(){
    	var _speed = 400; // in ms
    	var _hold = $('.detalis_top_right_4');
    	var _t = null;
    	if(_hold.length){
    		var _list = _hold.children();
    		_list.each(function(){
    			var _el = $(this);
    			var _box = _el.children('.detalis_top_right_5');
    			var _sub = _box.children();
    			_box.show().width(_sub.outerWidth()).hide();
    			_el.mouseenter(function(){
    			var _this = this;
    				_t = setTimeout(function(){
    					$(_this).css('zIndex', 10);
    					_el.addClass('hover');
    					if(_box.is(':hidden')){
    						_box.show();
    						_sub.css('marginTop', -_sub.outerHeight());
    					}
    					_sub.stop().animate({marginTop: 0}, _speed);
    				},0)
    			}).mouseleave(function(){
    				if(_t) clearTimeout(_t);
    				var _this = $(this);
    				$(this).css('zIndex', 5);
    				if(_sub.length){
    					_sub.stop().animate({marginTop: -_sub.outerHeight()}, _speed, function(){
    						_box.hide();
    						_el.removeClass('hover');
    						_this.css('zIndex', 1);
    					});
    				}else{
    					_el.removeClass('hover');
    				}
    			});
    		});
    	}
    }
</script>

